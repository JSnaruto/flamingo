{
  "extends": ["airbnb", "prettier"],
  "env": {
    "browser": true,
    "es6": true
  },
  "plugins": ["prettier"],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "jsx-a11y/label-has-for": [
      "error",
      {
        "required": {
          "every": ["id"]
        }
      }
    ],
    "react/jsx-filename-extension": 0,
    "react/jsx-closing-tag-location": 0,
    "react/jsx-indent": 0,
    "react/jsx-one-expression-per-line": 0
  }
}
